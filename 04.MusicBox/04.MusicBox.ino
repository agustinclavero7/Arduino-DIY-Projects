void animation(const byte PROGMEM frames[][512], int frameWidth, int frameHeight, int frameCount, int frameDelay ){  
  oledScreen.clearDisplay();
  oledScreen.drawBitmap(32, 0, frames[frame], frameWidth, frameHeight, 1);
  oledScreen.display();
  frame = (frame + 1) % frameCount;
  delay(frameDelay);
}

void setup() {
  Wire.begin();
  softSerial.begin(9600);
  oledScreen.begin (SSD1306_SWITCHCAPVCC,0x3C);
  pinMode(PLAY_BUTTON,INPUT_PULLUP);
//Presentación
  oledScreen.clearDisplay();
  oledScreen.setTextColor(WHITE);
  if(!mp3Player.begin(softSerial,true,false)){
    oledScreen.setCursor(10,32);
    oledScreen.print("FAIL");
    oledScreen.display();
    while(true);
  }
  else{
    for(int count = 0; count < DURATION; count ++)
      animation(musicNote,NOTE_WIDTH,NOTE_HEIGHT,NOTE_COUNT,NOTE_DELAY);
  }
}

void loop() {
  if(digitalRead(PLAY_BUTTON) == HIGH){
    oledScreen.clearDisplay();
    oledScreen.setTextColor(WHITE);
    oledScreen.setCursor(10,32);
    oledScreen.print("Press play");
    oledScreen.display();
  }
  else{
    oledScreen.clearDisplay();
    oledScreen.display();
  }
}
